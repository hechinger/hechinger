  <h4 class="quote-h">"hi.This is a big quote head for you to checkout right here"</h4>
  <h5 class="quote-sub-h">
    <a class="quote-link" href="#">xxxxx</a>
  </h5>
</div>

<!-- Recircuation Bottom -->
<!-- ===================================== -->
<div class="grid-container--rule">
   <div class="l-grid-2--rules">
      <section class="grid-item">
        <div class="grid-test"></div>
      </section> <!-- /grid-item -->

      <section class="grid-item">
        <div class="hp-latest-mod">
          <h4 class="hp-list-hdr">Latest News</h4>
          <ul class="hp-list-items">
            {% for i in 1..5 %}
            <li class="hp-list-item">
              <h3 class="hp-list-head">
                <a class="hp-list-link" href="#">This is a Latest News headline on the homepage</a>
              </h3>
            </li> <!-- /hp-list-item -->
            {% endfor %}
            <li class="hp-list-item">
              <h3 class="hp-list-head">
                <a class="hp-list-link" href="#">This is a <span class="head-highlight">Latest News headline</span> on the homepage</a>
              </h3>
            </li> <!-- /hp-list-item -->
          </ul> <!-- /hp-list-items -->
        </div> <!-- /hp-latest-mod -->
      </section> <!-- /grid-item -->

    </div> <!-- /l-grid-2 -->
</div> <!-- /grid-containter--rule -->
