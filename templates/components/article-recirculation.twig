  <div class="l-grid-3--rules">
    {% for i in 1..3 %}
    <section class="grid-item">
      <div class="grid-test"></div>
    </section> <!-- /grid-item -->
    {% endfor %}
  </div> <!-- /l-grid-3 -->
</div> <!-- /grid-container -->

<!-- Recirculation Middle -->
<!-- ===================================== -->
