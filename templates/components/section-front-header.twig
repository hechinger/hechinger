<header class="l-section-front-hdr-wrap l-page-hdr">
  <h1 class="archive-hdr">{{ section_front_title }}</h1>
</header>

{% if subhead %}
  <div class="section-front-summary-wrap">
    <p class="section-front-summary-text">{{ subhead }}
      {% if link_to_page %}
        <a class="section-front-summary-link" href="{{ link_to_page }}">{{ link_heading | default('Read more.') }}</a>
      {% endif %}
    </p>
  </div> <!-- /special-report-summary-wrap -->
{% endif %}
