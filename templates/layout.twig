<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <title>{% block title %}Hechinger Report{% endblock %}</title>
  <meta name="description" content="">
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />

  <link rel="stylesheet" href="/wp-content/themes/hechinger/static/css/screen.css">
  <script type="text/javascript" src="/wp-content/themes/hechinger/static/vendor/modernizr/modernizr.js"></script>
  {% block styles %}{# page-specific stylesheets #}{% endblock %}
</head>
<body>

  <header class="site-header">
    {% block navbar %}
      {% include 'components/nav-bar.twig' %}
    {% endblock %}
  </header> <!-- /site-header -->

  <section id="content" role="main" class="content-wrapper">
    {% block body %}{# page content goes here  #}{% endblock %}
  </section> <!-- /content content-wrapper -->

  {% include 'components/footer.twig' %}

  {% block scripts %}{# page-specific scripts #}{% endblock %}

  <script type="text/javascript" src="{{site.theme.uri}}/static/vendor/jquery/dist/jquery.js"></script>
  <script type="text/javascript" src="{{site.theme.uri}}/static/js/main.js"></script>
</body>
</html>
